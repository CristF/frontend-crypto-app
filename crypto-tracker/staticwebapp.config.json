{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*", "/css/*", "/static/*"]
    },
    "routes": [
        {
            "route": "/",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/login",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/register",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/home/*",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/list/*",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/database",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/search",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/verify-email",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}